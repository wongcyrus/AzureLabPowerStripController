{
  "name": "infrastructure",
  "author": "Cyrus Wong",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/wongcyrus/AzureLabPowerStripController",
    "type": "local"
  },
  "main": "src/main.js",
  "types": "src/main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "tsc && jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "build-jsii": "jsii",
    "build-jsii:watch": "jsii -w",
    "package-jsii": "jsii-pacmak"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "cdktf": "^0.11.0",
    "constructs": "^10.1.22"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "cdktf-cli": "^0.11.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "@types/prettier": "2.6.0"
  },
  "jsii": {    
    "excludeTypescript":[
      "*.d.ts",
      ".gen",     
      "cdktf.out"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "azure-cloud-lab",
        "module": "azure-cloud-lab.base"
      },
      "java": {
        "package": "com.cloudlabhk.azurecloudlab.jsii",
        "maven": {
          "groupId": "com.cloudlabhk.azurecloudlab.jsii",
          "artifactId": "jsii-azure-cloud-lab"
        }
      },
      "dotnet": {
        "namespace": "AzureCloudLab.Jsii",
        "packageId": "AzureCloudLab.Jsii"
      }
    }
  }
}